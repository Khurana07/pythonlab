# Creating a list of numbers
numbers = [10, 20, 30, 40, 50]

# Printing the list
print("Original List:", numbers)

# Accessing elements by index
print("First Element:", numbers[0])
print("Last Element:", numbers[-1])

# Modifying an element
numbers[1] = 25
print("List after modification:", numbers)

# Adding elements
numbers.append(60)  # Adds an element at the end
print("List after appending 60:", numbers)

numbers.insert(2, 35)  # Inserts 35 at index 2
print("List after inserting 35 at index 2:", numbers)

# Removing elements
numbers.remove(30)  # Removes the first occurrence of 30
print("List after removing 30:", numbers)

popped_value = numbers.pop()  # Removes the last element
print(f"Popped value: {popped_value}, List after popping:", numbers)

# Iterating over the list
print("Elements in the list:")
for num in numbers:
    print(num)

# Sorting the list
numbers.sort()
print("Sorted List:", numbers)

# Reversing the list
numbers.reverse()
print("Reversed List:", numbers)

# Finding the length of the list
print("Length of the list:", len(numbers))
